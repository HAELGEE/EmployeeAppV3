@{
    ViewBag.Title = "Alla anställda";
}
@model Employee[]

<ul>
    @foreach (var employee in Model)
    {
        <li><a asp-controller="Employee" asp-action="Details" asp-route-id="@employee.Id">@employee.Name</a></li>
    }
</ul>


<a asp-controller="employee" asp-action="Punchclock" >Stämpla In / Ut</a>

